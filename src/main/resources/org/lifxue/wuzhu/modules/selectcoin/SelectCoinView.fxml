<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<AnchorPane id="AnchorPane" prefHeight="700.0" prefWidth="850.0"
            xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="org.lifxue.wuzhu.modules.selectcoin.SelectCoinViewController">
    <children>
        <SplitPane dividerPositions="0.9" layoutX="87.0" layoutY="73.0" orientation="VERTICAL" prefHeight="700.0"
                   prefWidth="850.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                   AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <items>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="100.0" prefWidth="160.0">
                    <children>
                        <TableView fx:id="priceTable" editable="true" layoutX="98.0" layoutY="103.0" prefHeight="625.0"
                                   prefWidth="848.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                                   AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                            <columns>
                                <TableColumn fx:id="selectCol" prefWidth="423.0" text="选择"/>
                                <TableColumn fx:id="idCol" prefWidth="75.0" text="id"/>
                                <TableColumn fx:id="nameCol" prefWidth="423.0" text="名称"/>
                                <TableColumn fx:id="symbolCol" prefWidth="122.0" text="简称"/>
                                <TableColumn fx:id="rankCol" prefWidth="216.0" text="排名"/>
                                <TableColumn fx:id="dateCol" prefWidth="75.0" text="更新日期"/>
                            </columns>
                            <columnResizePolicy>
                                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
                            </columnResizePolicy>
                        </TableView>
                    </children>
                </AnchorPane>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="100.0" prefWidth="160.0">
                    <children>
                        <Button layoutX="749.0" layoutY="22.0" mnemonicParsing="false" onAction="#handleSave"
                                prefHeight="30.0" prefWidth="80.0" text="保  存" AnchorPane.bottomAnchor="20.0"
                                AnchorPane.rightAnchor="20.0"/>
                        <Label layoutX="278.0" layoutY="19.0" prefHeight="30.0" text="搜索："
                               AnchorPane.bottomAnchor="18.0">
                            <font>
                                <Font size="15.0"/>
                            </font>
                        </Label>
                        <TextField fx:id="searchField" layoutX="323.0" layoutY="19.0"
                                   onKeyReleased="#handleSearchFieldKeyReleased" prefHeight="30.0"
                                   promptText="填写coin简称" AnchorPane.bottomAnchor="18.0">
                            <font>
                                <Font size="15.0"/>
                            </font>
                        </TextField>
                    </children>
                </AnchorPane>
            </items>
        </SplitPane>
    </children>
</AnchorPane>
